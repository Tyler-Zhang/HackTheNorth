{
  "manifest_version": 2,
  "name": "Dressr",
  "version": "0.1",
  "content_scripts":
   [
        {
            "matches": ["*://*.google.ca/*"],
            "js": ["jquery-3.1.0.min.js","content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
         "default_popup": "popup.html",
         "default_title": "Review Your Wardrobe!"
    }
    
}